#include <iostream>
#include<cmath>
#include<stdio.h>
using namespace std;
 
int main() {
	int t;
	cin>>t;
	while(t>0)
	{
		int x,y;
		cin>>x>>y;
		if(x==y)
		{
			if(x%2==0)
			printf("%d\n",x*2);
			else
			printf("%d\n",x*2-1);
		}
		else if(x==(y+2))
		{
			if(x%2==0)
			printf("%d\n",x+y);
			else
			printf("%d\n",x+y-1);
		}
		else 
		printf("No Number\n");
		t--;
	}
 
	// your code here
 
	return 0;
} 
