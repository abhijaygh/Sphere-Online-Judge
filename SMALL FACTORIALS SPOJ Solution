#include <iostream>
#include<boost/multiprecision/cpp_int.hpp>
using boost::multiprecision::cpp_int;
using namespace std;
int main() {
	// your code goes here
	int n=0,t=0;
	cpp_int num;
	scanf("%d",&t);
	while(t>0)
	{
		num=1;
		scanf("%d",&n);
		for(int i=1;i<=n;i++)
			num=num*i;
		cout<<num<<endl;
		t--;
	}
	return 0;
} 
